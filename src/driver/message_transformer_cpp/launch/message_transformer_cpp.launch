<launch>
    <node name="qnx2ros" pkg="message_transformer_cpp" type="qnx2ros" output="screen" >
        <param name="filter_size" value="1" />
    </node>
    <node name="ros2qnx" pkg="message_transformer_cpp" type="ros2qnx" output="screen" >
        <param name="local_port" value="43894"/>
        <param name="remote_port" value="43893"/>
        <param name="remote_ip" value="192.168.1.120"/>
        <param name="packet_size" value="10"/>
    </node>
</launch>